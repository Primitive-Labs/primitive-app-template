{
  "name": "primitive-app-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm codegen && node scripts/dev-launch.mjs",
    "build": "pnpm codegen && pnpm format && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "clean-modules": "rm -rf node_modules pnpm-lock.yaml",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/",
    "codegen": "npx js-bao-codegen"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-bao": "git+https://github.com/Primitive-Labs/js-bao.git",
    "js-bao-wss-client": "git+https://github.com/Primitive-Labs/js-bao-wss-client.git",
    "lucide-vue-next": "^0.548.0",
    "pinia": "^3.0.4",
    "primitive-app": "^1.0.0",
    "reka-ui": "^2.6.0",
    "shadcn-vue": "^2.3.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.4",
    "wrangler": "^4.51.0"
  }
}
